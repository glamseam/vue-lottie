{
    "name": "@d-gs/vue-lottie",
    "version": "0.0.1",
    "description": "Vue component for Lottie",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "author": "__d",
    "license": "MIT",
    "keywords": [
        "vue",
        "vuejs",
        "component",
        "lottie"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/glamseam/vue-lottie.git",
        "directory": "package"
    },
    "scripts": {
        "build": "vue-tsc --declaration --emitDeclarationOnly --outDir dist && vite build",
        "vtsc": "vue-tsc --noEmit",
        "pub": "npm publish --access=public",
        "lint": "eslint src -c .eslintrc.json --ext ts,vue",
        "lint:fix": "eslint --fix src -c .eslintrc.json --ext ts,vue"
    },
    "peerDependencies": {
        "vue": "^3.2.0"
    },
    "dependencies": {
        "vue": "^3.2.40"
    },
    "devDependencies": {
        "@d-gs/eslint-config-for-vue": "^0.4.0",
        "@types/fs-extra": "^9.0.13",
        "@vitejs/plugin-vue": "^3.1.2",
        "fs-extra": "^10.1.0",
        "lottie-web": "^5.9.6",
        "vite": "^3.1.6",
        "vue-tsc": "^1.0.1"
    }
}
